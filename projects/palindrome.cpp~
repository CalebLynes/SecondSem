#include <iostream>
#include <cstring>

using namespace std;

int main() {
  char str[80];
  cin.get(str, 80);
  cin.get();

  cout << str << endl;
  int len = strlen(str);
  //cout << len << endl;
  
  int count = 0;
  for (int i = 0; i < len; i++) {
    if (str[i] != ' ' || str[i] != '\'' || str[i] != '.') {
      str[count++] = str[i];
    }
  }
  str[count] = '\0';

  cout << str << endl;
  //cout << strlen(str) << endl;
  
  return 0;
}
